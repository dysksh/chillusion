  <div class="row head">
    <div class="col-lg-2 col-lg-offset-1 col-md-3 col-md-offset-1 col-sm-4 col-xs-5">
      <div class="logo">
        <% if logged_in? %>
          <a class="navbar-left" href="/"><img src="/images/logo.png" width="50%" height="50%"></a>
        <% else %>
          <a class="navbar-left" href="/"><img src="/images/logo-top.png" width="150%" height="150%"></a>
        <% end %>
      </div>
    </div>
    <div class="col-lg-2 col-lg-offset-7 col-md-2 col-md-offset-6 col-sm-2 col-sm-offset-6 col-xs-3 col-xs-offset-4">
      <div class="menu">
        <% if logged_in? %>
          <% if current_page?(current_user) %>
            <%= link_to "Log out", logout_path, method: :delete %>
          <% else %>
            <%= link_to "My profile", user_path(current_user) %>
          <% end %>
        <% else %>
          <% if current_page?(login_path) %>
            <%= link_to "Sign up", signup_path %>
          <% else %>
            <%= link_to "Log in", login_path %>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
  
  
  
